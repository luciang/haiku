# FreeType 2 src/pfr Jamfile (c) 2002 David Turner
#

SubDir  OBOS_TOP src libs freetype2 pfr ;

UseFreeTypeHeaders ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = pfrdrivr  pfrgload  pfrload  pfrobjs pfrcmap pfrsbit ;
  }
  else
  {
    _sources = pfr ;
  }

  FT2_Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/pfr Jamfile
