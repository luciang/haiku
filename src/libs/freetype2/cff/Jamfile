# FreeType 2 src/cff Jamfile (c) 2001 David Turner
#

SubDir  OBOS_TOP src libs freetype2 cff ;

UseFreeTypeHeaders ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = cffdrivr cffgload cffload cffobjs cffparse cffcmap ;
  }
  else
  {
    _sources = cff ;
  }

  FT2_Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/cff Jamfile
