# FreeType 2 src/psaux Jamfile (c) 2001, 2002 David Turner
#

SubDir  OBOS_TOP src libs freetype2 psaux ;

UseFreeTypeHeaders ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = psauxmod psobjs t1decode t1cmap ;
  }
  else
  {
    _sources = psaux ;
  }

  FT2_Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/psaux Jamfile
