# FreeType 2 src/psnames Jamfile (c) 2001 David Turner
#

SubDir  OBOS_TOP src libs freetype2 psnames ;

UseFreeTypeHeaders ;

{
  local  _sources ;
  
  if $(FT2_MULTI)
  {
    _sources = psmodule ;
  }
  else
  {
    _sources = psnames ;
  }
  
  FT2_Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/psnames Jamfile
