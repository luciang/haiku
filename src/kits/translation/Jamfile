SubDir OBOS_TOP src kits translation ;

SharedLibrary translation :
	BitmapStream.cpp
	FuncTranslator.cpp
	TranslationUtils.cpp
	Translator.cpp
	TranslatorRoster.cpp
;

LinkSharedOSLibs libtranslation.so :
	be
;

MakeLocate <develop>libtranslation.so : $(OBOS_STLIB_DIR) ;
RelSymLink <develop>libtranslation.so : libtranslation.so ;

Package haiku-translationkit-cvs :
	README.html ;

Package haiku-translationkit-cvs :
	libtranslation.so :
	boot home config lib ;
