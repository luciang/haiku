SubDir OBOS_TOP src kits midi ;

UsePrivateHeaders midi ;

SharedLibrary midi :
	Midi.cpp
	MidiGlue.cpp
	MidiPort.cpp
	MidiStore.cpp
	MidiSynth.cpp
	MidiSynthFile.cpp
	MidiText.cpp
	Samples.cpp
	SoftSynth.cpp
	Synth.cpp
;

LinkSharedOSLibs libmidi.so :
	be
	midi2
;

MakeLocate <develop>libmidi.so : $(OBOS_STLIB_DIR) ;
RelSymLink <develop>libmidi.so : libmidi.so ;
