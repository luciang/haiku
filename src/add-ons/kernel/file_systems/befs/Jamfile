SubDir OBOS_TOP src add-ons kernel file_systems befs ;

{
	local debug = -g ;
	local defines = [ FDefines DEBUG ] ;
	SubDirCcFlags $(defines) $(debug) ;
	SubDirC++Flags $(defines) $(debug) ;
}

