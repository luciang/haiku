SubDir OBOS_TOP src add-ons kernel bus_managers ;

KernelObjects
	config_manager.c
	:
	-fno-pic -D_KERNEL_MODE
	;

KernelLd
	config_manager
	:
	<$(SOURCE_GRIST)>config_manager.o
	kernel.so
	:
	$(OBOS_TOP)/src/kernel/ldscripts/$(OBOS_ARCH)/add-on.ld
	:
	-Bdynamic -shared
	:
	:
	addons/kernel/config_manager
	;
SubInclude OBOS_TOP src add-ons kernel bus_managers usb ;

