	ac97.c
	debug.c
	auich.c
	io.c
	multi.c
	util.c
;

Package haiku-auich-cvs
	:
	auich
	:
	boot home config add-ons kernel drivers bin ;

PackageDriverSymLink haiku-auich-cvs : audio hmulti auich ;
