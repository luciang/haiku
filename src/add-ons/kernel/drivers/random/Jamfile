SubDir HAIKU_TOP src add-ons kernel drivers random ;

UsePrivateHeaders kernel [ FDirName kernel boot platform $(TARGET_BOOT_PLATFORM) ] ;
UseArchHeaders $(TARGET_ARCH) ;

KernelAddon random :
	driver.c
	;

