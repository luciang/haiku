SubDir OBOS_TOP src add-ons kernel file_cache ;

UsePrivateHeaders kernel ;
UsePrivateHeaders [ FDirName kernel arch $(OBOS_ARCH) ] ;
UsePrivateHeaders [ FDirName kernel boot platform $(OBOS_BOOT_PLATFORM) ] ;

KernelAddon log : kernel file_cache :
	log.cpp
	;

KernelAddon launch_speedup : kernel file_cache :
	launch_speedup.cpp
	;

KernelAddon rule_based_prefetcher : kernel file_cache :
	rule_based_prefetcher.cpp
	;

