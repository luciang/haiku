SubDir OBOS_TOP src add-ons print drivers pcl6 ;

SubDirHdrs [ FDirName $(OBOS_TOP) headers private print libprint ] ; 

AddResources PCL6\ Compatible : PCL6.rsrc ;

Addon PCL6\ Compatible : Print :
	DeltaRowCompression.cpp
	PCL6Entry.cpp
	PCL6.cpp
	PCL6Cap.cpp
	PCL6Rasterizer.cpp
	PCL6Writer.cpp
	Rasterizer.cpp
;

ObjectC++Flags [ FGristFiles jetlib.o ] : -w ;

LinkSharedOSLibs PCL6\ Compatible : be stdc++.r4 libprint.a ;

Package haiku-printingkit-cvs :
	PCL6\ Compatible :
	boot home config add-ons Print ;
