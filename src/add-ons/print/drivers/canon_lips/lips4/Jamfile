SubDir HAIKU_TOP src add-ons print drivers canon_lips lips4 ;

SetSubDirSupportedPlatformsBeOSCompatible ;

SubDirHdrs [ FDirName $(HAIKU_TOP) headers private print libprint ] ; 

AddResources Canon\ LIPS4\ Compatible : Lips4.rdef ;

Addon Canon\ LIPS4\ Compatible : Print :
	Lips4Entry.cpp
	Lips4.cpp
	Lips4Cap.cpp
;

LinkAgainst Canon\ LIPS4\ Compatible : be stdc++.r4 libprint.a ;

Package haiku-printingkit-cvs :
	Canon\ LIPS4\ Compatible :
	boot home config add-ons Print ;

