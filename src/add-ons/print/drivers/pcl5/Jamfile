SubDir HAIKU_TOP src add-ons print drivers pcl5 ;

SetSubDirSupportedPlatformsBeOSCompatible ;

SubDirHdrs [ FDirName $(HAIKU_TOP) headers private print libprint ] ; 

AddResources PCL5\ Compatible : PCL5.rsrc ;

Addon PCL5\ Compatible : Print :
	PCL5Entry.cpp
	PCL5.cpp
	PCL5Cap.cpp
;

LinkAgainst PCL5\ Compatible : be $(TARGET_LIBSTDC++) libprint.a ;

Package haiku-printingkit-cvs :
	PCL5\ Compatible :
	boot home config add-ons Print ;
