SubDir HAIKU_TOP src preferences sounds ;

SetSubDirSupportedPlatformsBeOSCompatible ;

if ! $(TARGET_PLATFORM_HAIKU_COMPATIBLE) {
	SubDirC++Flags -fmultiple-symbol-spaces ;
}

Preference Sounds : 
	HApp.cpp 
	HEventItem.cpp 
	HWindow.cpp 
	HEventList.cpp 
	: be game tracker libmedia.so
	: Sounds.rdef
	;
