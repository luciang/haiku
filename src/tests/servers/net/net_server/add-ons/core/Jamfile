Addon core : net_server/network : 
	cksum.c
	core.c
	if.c
	ifq.c
	in.c
	inpcb.c
	mbuf.c
	misc.c
	net_timer.c
	nhash.c
	pools.c
	radix.c
	route.c
	sockbuf.c
	socket.c
;

LinkSharedOSLibs core : root be _APP_ ;

