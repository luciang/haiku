SubDir OBOS_TOP src tests servers net net_server add-ons protocols tcp ;

UsePrivateHeaders net ;

SEARCH_SOURCE += [ FDirName $(OBOS_TOP) src add-ons kernel network protocols tcp ] ;

Addon tcp : net_server/network/protocols : 
	arc4random.c
	tcp.c
	tcp_debug.c
	tcp_input.c
	tcp_output.c
	tcp_timer.c
;

LinkSharedOSLibs tcp : root /system/servers/net_server ;

