SubDir OBOS_TOP src tests add-ons kernel network core ;

UsePrivateHeaders net ;

SEARCH_SOURCE += [ FDirName $(OBOS_TOP) src add-ons kernel network core ] ;

Addon <userland>core : userland network :
	cksum.c
	core.c
	if.c
	ifq.c
	in.c
	inpcb.c
	mbuf.c
	misc.c
	net_timer.c
	nhash.c
	pools.c
	radix.c
	route.c
	sockbuf.c
	socket.c
;

LinkSharedOSLibs <userland>core : <installed>net_stack_tester ;


