	mkdir.c
	mkfifo.c
	mknod.c
	nl.c
	od.c
	paste.c
	pathchk.c
	pr.c
	printenv.c
	printf.c
	ptx.c
	pwd.c
	readlink.c
	rmdir.c
	seq.c
