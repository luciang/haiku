SubDir OBOS_TOP src apps bin zip ;

StaticLibrary zip : 
	zipup.c
	crypt.c
	ttyio.c
	beos.c
	globals.c
	deflate.c
	fileio.c
	util.c
	crc32.c
	zipfile.c
	trees.c
	crctab.c
	: : zip.rdef
;

BinCommand zip :
	zip.c
	: libzip.a be : zip.rdef
;

BinCommand zipcloak :
	zipcloak.c
	bedefs.c
	: libzip.a : zip.rdef
;

BinCommand zipnote :
	zipnote.c
	bedefs.c
	: libzip.a be : zip.rdef
;

BinCommand zipsplit :
	zipsplit.c
	bedefs.c
	: libzip.a be : zip.rdef
;
