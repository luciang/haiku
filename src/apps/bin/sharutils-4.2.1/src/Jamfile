SubDir OBOS_TOP src apps bin sharutils-4.2.1 src ;

SubDirCcFlags += -I ../lib -DHAVE_CONFIG_H  -DLOCALEDIR=\'\"/share/locale\"\' ;

BinCommand shar : 
  	shar.c encode.c : root libsharutils.a ;

BinCommand unshar : 
  	unshar.c : root libsharutils.a ;

BinCommand uuencode : 
  	uuencode.c : root libsharutils.a ;

BinCommand uudecode : 
  	uudecode.c : root libsharutils.a ;
