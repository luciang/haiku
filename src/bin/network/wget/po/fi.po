msgid "done.    "
msgstr "valmis.  "

#: src/ftp.c:432 src/ftp.c:581 src/ftp.c:614 src/ftp.c:839 src/ftp.c:885
msgid "done.\n"
msgstr "valmis.\n"

#: src/ftp.c:459
#, c-format
