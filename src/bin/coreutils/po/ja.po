"  %l   hour ( 1..12)\n"
"  %m   month (01..12)\n"
"  %M   minute (00..59)\n"
msgstr ""
"  %k   時 ( 0〜23)\n"
"  %l   時 ( 1〜12)\n"
"  %m   月 (01〜12)\n"
"  %M   分 (00〜59)\n"

#: src/date.c:178
msgid ""
"  %n   a newline\n"
"  %N   nanoseconds (000000000..999999999)\n"
"  %p   locale's upper case AM or PM indicator (blank in many locales)\n"
"  %P   locale's lower case am or pm indicator (blank in many locales)\n"
"  %r   time, 12-hour (hh:mm:ss [AP]M)\n"
